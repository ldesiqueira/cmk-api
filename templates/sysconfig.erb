# The path where cmk-api is installed
PREFIX="<%= File.realpath(File.dirname(__FILE__)) %>"

# The name of the OMD site which runs check_mk
SITE="<%= @site %>"

# The port to listen on
PORT="<%= @port %>"

# The user name to run as
RUN_AS="<%= @site %>"

# The location of the Ruby binaries
RUBY_BINDIR="/opt/ruby/2.0.0/bin"
